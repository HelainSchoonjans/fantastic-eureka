    "input_file = \"input/input.xlsx\"\n",
    "\n",
    "df = pd.read_excel(input_file)\n",
    "\n",
    "comments = df[df.TypeAction == \"Comment\"]\n",
    "comments.drop(['Action_id', 'TypeAction'], 1,inplace=True)\n",
    "\n",
    "comments.head()"
   ]
